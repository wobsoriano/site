{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "baseUrl": ".",
    "strictNullChecks": true,
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "paths": {
      "@scripts/*": ["src/assets/scripts/*"],
      "@styles/*": ["src/assets/styles/*"],
      "@images/*": ["src/assets/images/*"],
    },
  }
}